package learnings_Selenium;

public class EclipsetoGit {

	public static void main(String[] args) {
		
		int a = 10;
		double b = a;
		System.out.println(b);
		
		String word = "Java";
		char w = word.charAt(0);
		System.out.println(w);
		
		int c = 5;
		int d = ++c;
		System.out.println(d);
		
		
		
	}

}
